                    <TextBox x:Name="tb_BT_Duration" IsEnabled="False" Padding="0" Background="#FF132226" Foreground="White" BorderBrush="White" FontFamily="Segoe UI Semibold" FontSize="14" HorizontalAlignment="Left" Height="28" Margin="151,106.8,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="72" Grid.Row="4" Grid.Column="1"/>
