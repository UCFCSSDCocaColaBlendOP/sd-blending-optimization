                    <Label x:Name="label_Equipment_Mix" Content="Equipment" Foreground="White" FontFamily="Segoe UI Semibold" FontSize="16" HorizontalAlignment="Left" Margin="241,94,0,0" VerticalAlignment="Top" Height="30" Width="140" Grid.Row="5"/>
                    <ComboBox x:Name="cb_Equipment_Mix" IsEnabled="False" Background="#FF132226" Foreground="White" BorderBrush="White" FontFamily="Segoe UI Semibold" FontSize="14" HorizontalAlignment="Left" Margin="386,96,0,0" Grid.Row="5" VerticalAlignment="Top" Width="170"/>
                    <Label x:Name="label_Inline_Mix" Content="Inline?" Foreground="White" FontFamily="Segoe UI Semibold" FontSize="16" HorizontalAlignment="Left" Margin="456,9,0,0" VerticalAlignment="Top" Height="30" Width="70" Grid.Row="5"/>
